{% extends 'base.html.twig' %}

{% block title %}Image{% endblock %}

{% block body %}
    <h1>Image</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ image.id }}</td>
            </tr>
            <tr>
                <th>Frame</th>
                <td>{{ image.frame }}</td>
            </tr>
            <tr>
                <th>FrontWheel</th>
                <td>{{ image.frontWheel }}</td>
            </tr>
            <tr>
                <th>Cockpit</th>
                <td>{{ image.cockpit }}</td>
            </tr>
            <tr>
                <th>Chainset</th>
                <td>{{ image.chainset }}</td>
            </tr>
            <tr>
                <th>RearWheel</th>
                <td>{{ image.rearWheel }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_image_index') }}">back to list</a>

    <a href="{{ path('app_image_edit', {'id': image.id}) }}">edit</a>

    {{ include('image/_delete_form.html.twig') }}
{% endblock %}
